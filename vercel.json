{
    "buildCommand": "cd client && npm install && npm run build",
    "outputDirectory": "client/dist",
    "devCommand": "cd client && npm run dev",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}